html
  head
    title=title
    link(
      href='//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css',
      rel='stylesheet'
    )
  body
    div.container
      div.page-header
        h1 Websites List
      table.table(data-toggle="table")
          thead
            tr
              th Object Name
              th Action
          tbody
            each object in objects
              tr 
                td #{object.name}
                td 
                  a(href='/objects/edit/#{object._id}') Edit
                  a(href='/objects/delete/#{object._id}', style='margin-left:10px;') Delete
      button.btn.btn-primary(type='submit', onclick='window.location.href="/objects/add"') Add new website
      div.pull-right
          a(href="/") Return to home page
                